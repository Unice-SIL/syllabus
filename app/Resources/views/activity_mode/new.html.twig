{% extends 'admin_layout.html.twig' %}
{% set h1 = 'admin.button.add_activity_mode' | trans %}
{% set href = path('app.admin.activity_mode.index') %}

{% block subContent %}

    <div class="card card-primary">
        <!-- /.card-header -->
        <!-- form start -->
        {{ form_start(form) }}
        <div class="card-body">
            {{ form_row(form.label) }}
