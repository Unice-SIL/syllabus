{
    "name": "unicesil/syllabus",
    "license": "proprietary",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Unice-SIL/linkify-bundle"
        },
        {
            "type": "vcs",
            "url": "https://github.com/Unice-SIL/SettingsBundle"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "api-platform/core": "^3.0",
        "dmishh/settings-bundle": "^6.4",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^3.0",
        "doctrine/orm": "^2.7",
        "exercise/htmlpurifier-bundle": "^5.0",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "guzzlehttp/guzzle": "^6.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/knp-paginator-bundle": "^5.3",
        "league/csv": "^9.5",
        "lexik/form-filter-bundle": "^7.0",
        "lexik/jwt-authentication-bundle": "^2.19",
        "lexik/translation-bundle": "^v6.0",
        "misd/linkify-bundle": "^6.4",
        "nelmio/cors-bundle": "^2.2",
        "peppeocchi/php-cron-scheduler": "^4.0",
        "phpdocumentor/reflection-docblock": "^5.4",
        "sensio/framework-extra-bundle": "^6.0.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "6.4.*",
        "symfony/console": "6.4.*",
        "symfony/dotenv": "6.4.*",
        "symfony/expression-language": "6.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/form": "6.4.*",
        "symfony/framework-bundle": "6.4.*",
        "symfony/ldap": "6.4.*",
        "symfony/mailer": "^6.4",
        "symfony/mime": "6.4.*",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/property-access": "6.4.*",
        "symfony/property-info": "6.4.*",
        "symfony/proxy-manager-bridge": "6.4.*",
        "symfony/runtime": "6.4.*",
        "symfony/security-bundle": "6.4.3",
        "symfony/serializer": "6.4.*",
        "symfony/templating": "6.4.*",
        "symfony/twig-bundle": "^6.4",
        "symfony/uid": "6.4.*",
        "symfony/validator": "6.4.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "tetranz/select2entity-bundle": "^3.0",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "unicesil/shibboleth-bundle": "^6.4"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "nikic/php-parser": "^4.18",
        "phpunit/phpunit": "^10.0",
        "rector/rector": "^1.2",
        "symfony/browser-kit": "6.4.*",
        "symfony/css-selector": "^6.4",
        "symfony/debug-bundle": "^6.4",
        "symfony/http-client": "6.4.*",
        "symfony/phpunit-bridge": "^6.4",
        "symfony/stopwatch": "^6.4",
        "symfony/var-dumper": "^6.4",
        "symfony/web-profiler-bundle": "^6.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        }
    },
    "config": {
        "platform": {
            "php": "8.2.14"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true,
            "symfony/runtime": true
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/packages/parameters.yaml"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "6.4.*"
        },
        "branch-alias": null
    }
}
