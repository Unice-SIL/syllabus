{
    "name": "unicesil/syllabus",
    "license": "proprietary",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Unice-SIL/linkify-bundle"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "api-platform/core": "^2.6",
        "dmishh/settings-bundle": "3.0.*@dev",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.7",
        "exercise/htmlpurifier-bundle": "^2.0",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "guzzlehttp/guzzle": "^6.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/knp-paginator-bundle": "^5.3",
        "league/csv": "^9.5",
        "lexik/form-filter-bundle": "^7.0",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nelmio/cors-bundle": "^2.2",
        "peppeocchi/php-cron-scheduler": "^3.0",
        "phpdocumentor/reflection-docblock": "v4.3.4",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.0.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/asset": "5.4.*",
        "symfony/console": "5.4.*",
        "symfony/dotenv": "5.4.*",
        "symfony/expression-language": "5.4.*",
        "symfony/flex": "^1.11",
        "symfony/form": "5.4.*",
        "symfony/framework-bundle": "5.4.*",
        "symfony/ldap": "5.4.*",
        "symfony/mime": "5.4.*",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/property-access": "5.4.*",
        "symfony/property-info": "5.4.*",
        "symfony/proxy-manager-bridge": "5.4.*",
        "symfony/security-bundle": "5.4.*",
        "symfony/serializer": "5.4.*",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/templating": "5.4.*",
        "symfony/twig-bundle": "^5.4",
        "symfony/validator": "5.4.*",
        "symfony/webpack-encore-bundle": "^1.1",
        "tetranz/select2entity-bundle": "^3.0",
        "twig/extensions": "^1.5",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/twig": "^2.12|^3.0",
        "unicesil/syllabus-apogee-importer-bundle": "^1.0"
    },
    "require-dev": {
        "dama/doctrine-test-bundle": "^6.7",
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/browser-kit": "5.4.*",
        "symfony/css-selector": "^5.4",
        "symfony/debug-bundle": "^5.4",
        "symfony/phpunit-bridge": "^5",
        "symfony/stopwatch": "^5.4",
        "symfony/var-dumper": "^5.4",
        "symfony/web-profiler-bundle": "^5.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "ckeditor:install": "symfony-cmd"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "symfony/flex": true
        }
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/packages/parameters.yaml"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "5.4.*"
        },
        "branch-alias": null
    }
}
