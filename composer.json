{
    "name": "unicesil/syllabus",
    "license": "proprietary",
    "type": "project",
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Unice-SIL/linkify-bundle"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "api-platform/api-pack": "^1.2",
        "dmishh/settings-bundle": "3.0.*@dev",
        "doctrine/doctrine-bundle": "^1.6",
        "doctrine/doctrine-migrations-bundle": "^2.0",
        "doctrine/orm": "^2.5",
        "exercise/htmlpurifier-bundle": "^2.0",
        "friendsofsymfony/ckeditor-bundle": "^2.0",
        "guzzlehttp/guzzle": "^6.5",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/knp-paginator-bundle": "4.1.1",
        "league/csv": "^9.5",
        "lexik/form-filter-bundle": "v5.0.10",
        "lexik/jwt-authentication-bundle": "^2.6",
        "lexik/translation-bundle": "^4.0",
        "misd/linkify-bundle": "dev-master",
        "peppeocchi/php-cron-scheduler": "^3.0",
        "phpdocumentor/reflection-docblock": "v4.3.4",
        "ramsey/uuid": "^3.8",
        "sensio/framework-extra-bundle": "^5.0.0",
        "stof/doctrine-extensions-bundle": "^1.3",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.11",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/monolog-bundle": "^3.5.0",
        "symfony/orm-pack": "^2.1",
        "symfony/polyfill-apcu": "^1.0",
        "symfony/security-bundle": "4.4.*",
        "symfony/swiftmailer-bundle": "^3.5",
        "symfony/twig-pack": "^1.0",
        "symfony/validator": "4.4.*",
        "tanigami/specification": "^1.3",
        "tetranz/select2entity-bundle": "^2.9",
        "twig/extensions": "^1.5",
        "unicesil/shibboleth-bundle": "^1.0",
        "unicesil/syllabus-apogee-importer-bundle": "^1.0"
    },
    "require-dev": {
        "doctrine/doctrine-fixtures-bundle": "^3.1",
        "symfony/phpunit-bridge": "^3",
        "symfony/webpack-encore-bundle": "^1.1"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters"
        ],
        "post-install-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts",
            "@auto-scripts"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        }
    },
    "config": {
        "platform": {
            "php": "7.2.5"
        },
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "config/packages/parameters.yaml"
        },
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        },
        "branch-alias": null
    }
}
